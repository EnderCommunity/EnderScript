{
    "name": "enderscript",
    "displayName": "%displayName%",
    "description": "%description%",
    "version": "0.0.1",
    "publisher": "EnderCommunity",
    "license": "MIT",
    "engines": {
        "vscode": "0.10.x"
    },
    "scripts": {
        "update-grammar": "node ../node_modules/vscode-grammar-updater/bin dotnet/csharp-tmLanguage grammars/csharp.tmLanguage ./syntaxes/csharp.tmLanguage.json"
    },
    "contributes": {
        "languages": [{
            "id": "enderscript",
            "extensions": [
                ".esf"
            ],
            "aliases": [
                "EnderScript",
                "enderscript",
                "ES",
                "es",
                "CaPDSL-2021"
            ],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "enderscript",
            "scopeName": "source.cs",
            "path": "./syntaxes/csharp.tmLanguage.json"
        }],
        "snippets": [{
            "language": "enderscript",
            "path": "./snippets/csharp.code-snippets"
        }]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/EnderCommunity/EnderScript.git"
    }
}